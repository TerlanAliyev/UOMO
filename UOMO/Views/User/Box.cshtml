@model List<Box>

<main>
    <div class="mb-4 pb-4"></div>
    <section class="my-account container m-auto">
        <h2 class="page-title">Orders</h2>
        <div class="row ">
            <div class="col-lg-12">
                <div class="page-content my-account__orders-list">
                    <table class="orders-table">
                        <thead>
                            <tr>
                                <th>Photo</th>
                                <th>Name</th>
                                <th>Color</th>
                                <th>Size</th>
                                <th>Price</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td><img style="height:100px" src=" /images/products/@item.BoxProduct.Photos.ToList()[0].PhotoUrl" alt="Alternate Text" /></td>
                                    <td>@item.BoxProduct.ProductName</td>
                                    <td>@item.BoxColor</td>
                                    <td>@item.BoxSize</td>
                                    <td>@item.BoxTotalPrice</td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
</main>

<div class="mb-5 pb-xl-5"></div>